<h1>New list</h1>

<% form_for([@user, @list]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label Time.parse('EST') %><br />
    
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Cancelar', user_lists_path(@user) %>

